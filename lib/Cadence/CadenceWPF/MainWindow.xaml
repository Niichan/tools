<Window x:Class="CadenceWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Label" Height="27" HorizontalAlignment="Left" Margin="12,12,0,0" Name="SongName" VerticalAlignment="Top" Width="215" />
        <Label Content="Label" Height="26" HorizontalAlignment="Left" Margin="12,45,0,0" Name="ArtistName" VerticalAlignment="Top" Width="215" />
        <TextBox Height="22" HorizontalAlignment="Left" Margin="12,277,0,0" Name="searchBox" VerticalAlignment="Top" Width="398" />
        <Button Content="Search" Height="23" HorizontalAlignment="Left" Margin="416,276,0,0" Name="SearchButton" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <WebBrowser Height="259" HorizontalAlignment="Left" Margin="232,0,0,40" Name="AlbumArt" VerticalAlignment="Bottom" Width="259" DataContext="{Binding}" />
    </Grid>
</Window>
